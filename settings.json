            "server": "FINTECHDB01-DEV,50003",
            "trustServerCertificate": true,
            "groupId": "730D9075-F16C-4D51-BD38-D51C5C466A8F",
            "id": "72BA7F69-C479-479A-AE0D-78F8BCDC6ECF",
            "database": "",
            "user": "",
            "password": "",
            "commandTimeout": 30,
            "encrypt": "Mandatory",
            "profileSource": 0
        }
    ],
    "[sql]": {
        "editor.defaultFormatter": "ms-mssql.mssql"
    },
    "sqltools.highlightQuery": false,
    "editor.minimap.enabled": false,
    "workbench.activityBar.location": "hidden"
}